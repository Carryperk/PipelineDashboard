{
  "dynamodb": {
    "environments": {
      "table": "pipelinedashboard-environments"
    },
    "deployed": {
      "table": "pipelinedashboard-deployed"
    }
  },
  "offline": {
    "region": "localhost",
    "endpoint": "http://localhost:8000"
  },
  "envars": {
    "AUTH0_CLIENT_ID": "{{ AUTH0_CLIENT_ID }}",
    "AUTH0_CLIENT_SECRET": "{{ AUTH0_CLIENT_SECRET }}"
  }
}
